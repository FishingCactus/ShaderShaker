float multiplied_by_0_r( float a, float b )
{
	return a * b * 0.0f;
}

float multiplied_by_0_l( float a, float b )
{
	return 0.0f * a * b;
}

float multiplied_by_0_m( float a, float b )
{
	return a * 0.0f * b;
}

float multiplied_by_1_r( float a, float b )
{
	return a * b * 1.0f;
}

float multiplied_by_1_l( float a, float b )
{
	return 1.0f * a * b;
}

float multiplied_by_1_m( float a, float b )
{
	return a * 1.0f * b;
}